{
  "name": "joke-machine-api",
  "version": "1.0.0",
  "description": "Joke website using TypeScript, SASS, Bootstrap and APIs",
  "main": "index.js",
  "scripts": {
    "start": "lite-server",
    "build": "tsc",
    "build:sass": "sass src/scss/main.scss dist/css/main.css",
    "watch:sass": "sass --watch src/scss:dist/css",
    "watch:ts": "tsc --watch",
    "dev": "npm-run-all --parallel watch:sass watch:ts start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VictorGonPe/Joke-machine-API.git"
  },
