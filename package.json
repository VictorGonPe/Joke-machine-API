  "bugs": {
    "url": "https://github.com/VictorGonPe/Joke-machine-API/issues"
  },
  "homepage": "https://github.com/VictorGonPe/Joke-machine-API#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  }
}
